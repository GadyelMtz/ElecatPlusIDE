programa #luces#{
	entero #variable1#=0;
	#luces#(){
		display_lcd #display#;
		led #ledIzquierda# = 8;
		led #ledDerecha# = 9;
		buzzer #claxon# = 12;
		sensor_ultrasonico #sensor# = 10;
		boton #botonera# = 3;
	}

	ejecutar(){
		accion(#ledIzquierda#, encender());
		esperar(1500);
		accion(#ledDerecha#, encender());
		esperar(1500);
		accion(#ledIzquierda#, apagar());
		esperar(1500);
		accion(#ledDerecha#, apagar());
		esperar(1500);


	}
		


}
